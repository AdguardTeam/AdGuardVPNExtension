{
    "name": "adguard-vpn",
    "version": "0.7.12",
    "description": "AdGuard VPN",
    "author": "adguard@adguard.com",
    "license": "LGPL-3.0",
    "scripts": {
        "lint": "eslint .",
        "watch": "BUILD_ENV=dev run-p \"build:chrome --watch\" \"build:firefox --watch\"",
        "watch:firefox": "yarn build:firefox --watch",
        "watch:chrome": "yarn build:chrome --watch",
        "dev": "BUILD_ENV=dev run-p build:*",
        "beta": "BUILD_ENV=beta run-p build:*",
        "release": "BUILD_ENV=release run-p build:*",
        "build:chrome": "BROWSER=chrome webpack --config tasks/chrome/webpack.chrome.js",
        "build:firefox": "BROWSER=firefox webpack --config tasks/firefox/webpack.firefox.js",
        "locales:download": "LOCALES=DOWNLOAD node tasks/locales.js",
        "locales:upload": "LOCALES=UPLOAD node tasks/locales.js",
        "test": "jest",
        "test:watch": "jest --watch",
        "compile-proto": "./tasks/compile-protobuf.sh",
        "ci:increment": "yarn version --patch --no-git-tag-version",
        "crx:beta": "export BUILD_ENV=beta; yarn build:chrome && node tasks/crx.js",
        "xpi:beta": "export BUILD_ENV=beta; yarn build:firefox && node tasks/xpi.js",
        "crx:release": "export BUILD_ENV=release; yarn build:chrome && node tasks/crx.js",
        "xpi:release": "export BUILD_ENV=release; yarn build:firefox && node tasks/xpi.js",
        "artifacts:beta": "yarn crx:beta && yarn xpi:beta",
        "artifacts:release": "yarn crx:release && yarn xpi:release"
    },
    "husky": {
        "hooks": {
            "pre-commit": "yarn lint",
            "pre-push": "yarn test"
        }
    },
    "devDependencies": {
        "@babel/cli": "^7.8.4",
        "@babel/core": "^7.8.4",
        "@babel/plugin-proposal-class-properties": "^7.8.3",
        "@babel/plugin-proposal-decorators": "^7.8.3",
        "@babel/plugin-proposal-export-default-from": "^7.8.3",
        "@babel/plugin-proposal-export-namespace-from": "^7.8.3",
        "@babel/plugin-proposal-nullish-coalescing-operator": "^7.8.3",
        "@babel/plugin-proposal-optional-chaining": "^7.8.3",
        "@babel/plugin-transform-runtime": "^7.8.3",
        "@babel/preset-env": "^7.8.4",
        "@babel/preset-react": "^7.8.3",
        "axios": "^0.19.2",
        "babel-eslint": "^10.0.3",
        "babel-loader": "^8.0.6",
        "chalk": "^4.0.0",
        "clean-webpack-plugin": "^3.0.0",
        "copy-webpack-plugin": "^5.1.1",
        "create-file-webpack": "^1.0.2",
        "crx": "^5.0.1",
        "css-loader": "^3.4.2",
        "dotenv": "^8.2.0",
        "eslint": "^6.8.0",
        "eslint-config-airbnb": "^18.0.1",
        "eslint-plugin-import": "^2.20.1",
        "eslint-plugin-jsx-a11y": "^6.2.3",
        "eslint-plugin-react": "^7.18.3",
        "file-loader": "^5.0.2",
        "form-data": "^3.0.0",
        "html-webpack-plugin": "^3.2.0",
        "husky": "^4.2.1",
        "jest": "^25.1.0",
        "loader-utils": "^1.2.3",
        "npm-run-all": "^4.1.5",
        "pac-resolver": "^4.0.0",
        "postcss-import": "^12.0.1",
        "postcss-loader": "^3.0.0",
        "postcss-nested": "^4.2.1",
        "postcss-preset-env": "^6.7.0",
        "postcss-svg": "^3.0.0",
        "schema-utils": "^2.6.4",
        "sinon-chrome": "^3.0.1",
        "style-loader": "^1.1.3",
        "tmp-promise": "^2.0.2",
        "web-ext": "^4.2.0",
        "webpack": "^4.41.5",
        "webpack-cli": "^3.3.10",
        "webpack-merge": "^4.2.2",
        "ws": "^7.2.5",
        "zip-webpack-plugin": "^3.0.0"
    },
    "dependencies": {
        "@babel/runtime": "^7.8.4",
        "classnames": "^2.2.6",
        "crypto-js": "^3.1.9-1",
        "geolib": "^3.2.1",
        "ipaddr.js": "^1.9.1",
        "lodash": "^4.17.15",
        "mobx": "^5.15.4",
        "mobx-react": "^6.1.8",
        "nanoid": "^2.1.11",
        "prop-types": "^15.7.2",
        "protobufjs": "^6.8.8",
        "qs": "^6.9.1",
        "react": "^16.12.0",
        "react-dom": "^16.12.0",
        "react-html-parser": "^2.0.2",
        "react-modal": "^3.11.1",
        "react-router-dom": "^5.1.2",
        "react-simple-maps": "^0.12.1",
        "react-transition-group": "^4.3.0",
        "reconnecting-websocket": "^4.3.0",
        "webextension-polyfill": "^0.6.0"
    }
}
