@keyframes notify-show {
    0% {
        transform: translateY(200px);
    }

    100% {
        transform: translateY(0);
    }
}

@keyframes notify-show-reverse {
    0% {
        transform: translateY(0);
    }

    100% {
        transform: translateY(200px);
    }
}

.holiday-notify {
    position: fixed;
    left: 0;
    right: 0;
    bottom: 0;
    height: 108px;

    &__wrap {
        bottom: 0;
        left: 0;
        position: absolute;
        z-index: 2;
        width: 100%;
        outline: none;
        animation: notify-show 0.3s ease-in
    }

    &__close {
        top: 22px;
        right: 0;
        position: absolute;
        z-index: 2;
        width: 24px;
        height: 24px;
        background-size: cover;
        background-repeat: no-repeat;
        background-image: url(data:image/png;base64,);
        cursor: pointer;
    }

    &__content {
        width: 100%;
        min-height: 144px;
        position: relative;
        background-position: 0px 37px;
        background-size: contain;
        background-repeat: no-repeat;
        background-image: url(data:image/png;base64,);
    }

    &__bottom {
        padding: 0 16px;
        max-width: 210px;
        display: flex;
        justify-content: flex-start;
    }

    &__title {
        color: #131313;
        font-weight: 700;
        line-height: 25px;
        padding: 67px 15px 8px 15px;
        font-size: 20px;
    }

    &__title-in {
        white-space: pre-wrap;
    }

    &__btn {
        text-align: center;
        position: relative;
        z-index: 2;
        cursor: pointer;
        display: inline-block;
        color: #fff;
        font-size: 14px;
        font-weight: 500;
        background-color: #4a99e9;
        line-height: 28px;
        padding: 0 8px;
        min-width: 120px;
        border: 0;
        border-radius: 8px;
    }
}
