@keyframes notify-show {
    0% {
        transform: translateY(200px);
    }

    100% {
        transform: translateY(0);
    }
}

@keyframes notify-show-reverse {
    0% {
        transform: translateY(0);
    }

    100% {
        transform: translateY(200px);
    }
}

.holiday-notify {
    pointer-events: none;
    position: fixed;
    left: 0;
    right: 0;
    bottom: 0;
    height: 108px;

    &__wrap {
        bottom: 0;
        left: 0;
        position: absolute;
        z-index: 2;
        width: 100%;
        outline: none;
        animation: notify-show 0.3s ease-in
    }

    &__inner {
        width: 100%;
        height: 140px;
        pointer-events: auto;
        position: absolute;
        bottom: 0;
        left: 0;
    }

    &__content {
        pointer-events: none;
        width: 100%;
        min-height: 148px;
        position: relative;
        background-size: cover;
        background-repeat: no-repeat;
        text-align: center;
        background-image: url(data:image/png;base64,);
    }

    &__close {
        pointer-events: auto;
        top: 0;
        right: 10px;
        position: absolute;
        z-index: 2;
        width: 24px;
        height: 24px;
        background-size: cover;
        background-repeat: no-repeat;
        background-image: url(data:image/png;base64,);
        cursor: pointer;
    }

    &__bottom {
        padding: 0 16px;
        display: flex;
        justify-content: center;
    }

    &__title {
        color: #fff;
        font-weight: 700;
        line-height: 24px;
        padding: 16px 15px 16px 15px;
        font-size: 22px;
    }

    &__title-in {
        white-space: pre-wrap;
    }

    &__btn {
        text-align: center;
        position: relative;
        z-index: 2;
        cursor: pointer;
        display: inline-block;
        color: #4D4D4D;
        font-weight: 500;
        font-size: 16px;
        line-height: 36px;
        background-color: #fff;
        padding: 0 32px;
        min-width: 128px;
        border: 0;
        box-shadow: 4px 4px 12px rgba(0, 0, 0, 0.15);
        border-radius: 40px;
    }
}
