@keyframes notify-show {
    0% {
        transform: translateY(200px);
    }

    100% {
        transform: translateY(0);
    }
}

@keyframes notify-show-reverse {
    0% {
        transform: translateY(0);
    }

    100% {
        transform: translateY(200px);
    }
}

.holiday-notify {
    position: fixed;
    left: 0;
    right: 0;
    bottom: 0;
    height: 108px;
    background-color: var(--white);

    &__wrap {
        bottom: 0;
        left: 0;
        position: absolute;
        z-index: 2;
        width: 100%;
        outline: none;
        animation: notify-show 0.3s ease-in;
    }

    &__close {
        top: -15px;
        right: 0;
        position: absolute;
        z-index: 2;
        width: 32px;
        height: 32px;
        background-size: cover;
        background-repeat: no-repeat;
        background-image: url(data:image/png;base64,);
        cursor: pointer;
    }

    &__content {
        width: 100%;
        min-height: 125px;
        position: relative;
        background-image: url(data:image/jpeg;base64,);
        background-size: cover;
    }

    &__bottom {
        padding: 0 15px;
    }

    &__title {
        text-align: center;
        color: #fff;
        font-weight: 300;
        line-height: 28px;
        padding: 14px 12px 18px 12px;
        font-size: 28px;
        text-shadow: -0.5px -0.5px 0 #e25959, 0.5px -0.5px 0 #e25959, -0.5px 0.5px 0 #e25959, 0.5px 0.5px 0 #e25959, 0 0 5px #e25959, 0px 5px 10px #e25959;
    }

    &__title-in {
        white-space: pre-wrap;
    }

    &__btn {
        text-align: center;
        position: relative;
        z-index: 2;
        cursor: pointer;
        display: inline-block;
        color: #fff;
        font-size: 15px;
        font-weight: bold;
        background-color: #FC119E;
        font-weight: bold;
        padding: 10px 19px;
        min-width: 130px;
        border: 1px solid #fff;
        border-radius: 50px;

        &:hover {
            background-color: #F2159A;
        }
    }
}
