@keyframes notify-show {
    0% {
        transform: translateY(200px);
    }

    100% {
        transform: translateY(0);
    }
}

@keyframes notify-show-reverse {
    0% {
        transform: translateY(0);
    }

    100% {
        transform: translateY(200px);
    }
}

.holiday-notify {
    position: fixed;
    left: 0;
    right: 0;
    bottom: 0;
    height: 108px;
    background-color: var(--white);

    &__wrap {
        bottom: 0;
        left: 0;
        position: absolute;
        z-index: 2;
        width: 100%;
        outline: none;
        animation: notify-show 0.3s ease-in;
    }

    &__close {
        top: 5px;
        right: 0;
        position: absolute;
        z-index: 2;
        width: 24px;
        height: 24px;
        background-repeat: no-repeat;
        background-image: url("data:image/svg+xml;base64,");
        cursor: pointer;
    }

    &__content {
        width: 100%;
        position: relative;
        box-shadow: 0 0 32px rgba(0, 0, 0, 0.25);
        background-image: url("data:image/svg+xml;base64,");

        &:after {
            content: "";
            position: absolute;
            z-index: 1;
            left: 0;
            top: 0;
            width: 135px;
            height: 120px;
            background-size: cover;
            background-image: url("data:image/svg+xml;base64,");
        }
    }

    &__bottom {
        padding: 0 25px 15px 0;
        text-align: right;
    }

    &__title {
        color: var(--white);
        padding: 20px 5px 13px 126px;
        font-weight: 500;
        font-size: 20px;
        line-height: 23px;
        text-shadow: 0 5px 10px #2A2A2A;
    }

    &__title-in {
        text-align: left;
        white-space: pre-wrap;
    }

    &__btn {
        text-align: center;
        position: relative;
        z-index: 2;
        cursor: pointer;
        display: inline-block;
        background-color: #ffe68f;
        color: #2a2a2a;
        font-size: 14px;
        font-weight: 400;
        padding: 7px 16px;
        min-width: 168px;
        border: 0;
        border-radius: 8px;
        box-shadow: 0 2px 6px 0 rgba(0, 0, 0, 0.5);

        &:hover {
            background-color: #e2ca75;
        }
    }
}
