    &__bottom {
        padding: 13px 25px 14px 0;
        text-align: right;
        background: radial-gradient(434.54% 4116.41% at 17.5% 209.82%, #5A5F65 0%, #333B43 100%);
    }

    &__title {
        color: #fff;
        padding: 13px 5px 13px 90px;
        font-weight: 500;
        font-size: 16px;
        line-height: 21px;
    }

    &__title-in {
        text-align: center;
    }

    &__btn {
        text-align: center;
        position: relative;
        text-transform: uppercase;
        z-index: 2;
        cursor: pointer;
        display: inline-block;
        background-color: #ffe68f;
        color: #2a2a2a;
        font-size: 14px;
        font-weight: 700;
        padding: 7px 16px;
        min-width: 168px;
        border: 0;
        border-radius: 8px;
        box-shadow: 0 2px 6px 0 rgba(0, 0, 0, 0.5);

        &:hover {
            background-color: #e2ca75;
        }
    }
}
