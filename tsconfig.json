{
  "compilerOptions": {
    "baseUrl": "src",
    "target": "esnext",
    "jsx": "react",
    "module": "ESNext",
    "moduleResolution": "Node",
    "experimentalDecorators": true,
    "allowJs": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "noImplicitAny": true,
  }
}
